{
  "name": "angular-phonecat",
  "private": true,
  "version": "0.0.0",
  "description": "A tutorial application for AngularJS",
  "scripts": {
    "start": "node index.js",
    "start-prod": "http-server ./src -a localhost -p 8000 -c-1",
    "cypress:record": "cypress run --record",
    "cypress:run": "cypress run",
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "postinstall": "bower install --allow-root",
    "test": "karma start karma.conf.js",
    "test:single:run": "karma start karma.conf.js --single-run",
    "test:ci": "run-p --race start:quiet cypress:run",
    "record:ci": "run-p --race start:quiet cypress:record",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "update-index-async": "node -e \"var fs=require('fs'),indexFile='src/index-async.html',loaderFile='src/bower_components/angular-loader/angular-loader.min.js',loaderText=fs.readFileSync(loaderFile,'utf-8').split(/sourceMappingURL=angular-loader.min.js.map/).join('sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map'),indexText=fs.readFileSync(indexFile,'utf-8').split(/\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/).join('//@@NG_LOADER_START@@\\n'+loaderText+'    //@@NG_LOADER_END@@');fs.writeFileSync(indexFile,indexText);\""
  },
  "dependencies": {
    "bower": "^1.7.7",
    "http-server": "^0.11.1",
    "live-server": "^1.2.1",
    "npm-run-all": "4.1.3"
  },
  "devDependencies": {
    "cypress": "3.2.0",
    "jasmine-core": "^3.0.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.1",
    "protractor": "^5.2.1"
  }
}
